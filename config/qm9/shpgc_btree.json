{
    "dataset": "qm9",
    "model": "sparse_hpgc",
    "order": "canonical",
    "model_hpars": {
        "backend": "btree",
        "b_hpars": {
            "nl": 5,
            "ns": 32,
            "ni": 32
        },
        "device": "cuda"
    },
    "optimizer": "adam",
    "optimizer_hpars": {
        "lr": 0.05,
        "betas": [
            0.9,
            0.82
        ]
    },
    "num_epochs": 10,
    "batch_size": 256,
    "seed": 0
}